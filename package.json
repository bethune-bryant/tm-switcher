{
  "name": "tm-switcher",
  "version": "1.10.1",
  "main": "src/main.ts",
  "bin": "out/switcher/main.cjs",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "switcher:build": "rollup -c",
    "switcher:run": "deno -A out/switcher/main.cjs",
